.content__default:not(.custom) > h1{
    display: none;
    padding-top:0rem;
}

.dropdown-wrapper .nav-dropdown .dropdown-item .dropdown-subitem-wrapper .dropdown-subitem {
    font-size: 0.8em!important;
}

.dropdown-wrapper .nav-dropdown .dropdown-item {
    margin-bottom: 0.2em;
}

.dropdown-wrapper .nav-dropdown .dropdown-item h4 {
    margin-bottom: 0.45rem!important;
}

h1, h2, h3, h4, h5, h6 {
    font-weight: 600 !important;
}

body,td,th {
    font-family: "LXGW WenKai",Consolas, "微软雅黑", "Liberation Mono", Menlo, Courier, monospace !important;
}

code {
    font-family: "Source Code Pro" , "微软雅黑", "Liberation Mono", Menlo, Courier, monospace !important;
}


/* source-code-pro-regular */
@font-face {
  font-family: 'Source Code Pro';
  font-style: normal;
  font-weight: regular;
  src: url('//lib.baomitu.com/fonts/source-code-pro/source-code-pro-regular.eot'); /* IE9 Compat Modes */
  src: local('Source Code Pro'), local('SourceCode Pro-Normal'),
       url('//lib.baomitu.com/fonts/source-code-pro/source-code-pro-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('//lib.baomitu.com/fonts/source-code-pro/source-code-pro-regular.woff2') format('woff2'), /* Super Modern Browsers */
